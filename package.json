{
  "name": "guddoy.dev",
  "version": "1.0.0",
  "description": "Sacha Guddoy's Website",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src",
    "test:ci": "npm run test -- --ci --collect-coverage",
    "test:watch": "jest ./src --watch",
    "test-ua": "npm run start > /dev/null | CYPRESS_baseUrl=http://localhost:3000 cypress open",
    "test-ua:ci": "netlify deploy --json > deploy.json && CYPRESS_baseUrl=$(jq .deploy_url deploy.json | tr -d '\"') cypress run",
    "dev": "next ./src",
    "build": "next build ./src",
    "start": "npm run export && http-server ./out -p 3000 ",
    "export": "npm run build && next export ./src --outdir='./out'",
    "lint": "eslint ./src ./cypress --fix"
  },
  "author": "Sacha Guddoy",
  "license": "ISC",
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.1.0",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@material-ui/core": "^4.0.0-rc.0",
    "contentful": "^7.8.2",
    "dateformat": "^3.0.3",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.13",
    "material-design-icons": "^3.0.1",
    "next": "^8.1.0",
    "next-images": "^1.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "cypress": "3.4.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "jest-junit": "^7.0.0",
    "netlify": "^2.4.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/out/**",
      "!**/dist/**",
      "!**/.next/**"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": ["jest-junit"]
  }
}
